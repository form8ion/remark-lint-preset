{
  "name": "@form8ion/remark-lint-preset",
  "description": "remark preset to configure remark-lint with markdown style conventions",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": ">12.17"
  },
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "repository": "form8ion/remark-lint-preset",
  "bugs": "https://github.com/form8ion/remark-lint-preset/issues",
  "homepage": "https://npm.im/@form8ion/remark-lint-preset",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark --frail .",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@form8ion/commitlint-config": "1.0.30",
    "@form8ion/eslint-config": "2.0.3",
    "@travi/any": "2.0.19",
    "ban-sensitive-files": "1.9.18",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "npm-run-all": "4.1.5",
    "remark-cli": "10.0.1"
  },
  "dependencies": {
    "remark-gfm": "^3.0.0",
    "remark-lint-final-definition": "3.1.1",
    "remark-lint-list-item-indent": "3.1.1",
    "remark-lint-maximum-line-length": "3.1.2",
    "remark-lint-no-empty-url": "3.1.1",
    "remark-lint-no-tabs": "3.1.1",
    "remark-lint-ordered-list-marker-value": "3.1.1",
    "remark-lint-unordered-list-marker-style": "3.1.1",
    "remark-preset-lint-recommended": "6.1.2",
    "remark-validate-links": "11.0.2"
  }
}
