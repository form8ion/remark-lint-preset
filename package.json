{
  "name": "@form8ion/remark-lint-preset",
  "description": "remark preset to configure remark-lint with markdown style conventions",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js"
  ],
  "repository": "form8ion/remark-lint-preset",
  "bugs": "https://github.com/form8ion/remark-lint-preset/issues",
  "homepage": "https://npm.im/@form8ion/remark-lint-preset",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:js": "eslint . --cache",
    "lint:md": "remark --frail .",
    "lint:sensitive": "ban",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@form8ion/commitlint-config": "1.0.16",
    "@form8ion/eslint-config": "1.7.8",
    "@travi/any": "2.0.16",
    "ban-sensitive-files": "1.9.15",
    "cz-conventional-changelog": "3.3.0",
    "husky": "5.1.3",
    "npm-run-all": "4.1.5",
    "remark-cli": "9.0.0"
  },
  "dependencies": {
    "remark-lint-final-definition": "2.1.0",
    "remark-lint-list-item-indent": "2.0.1",
    "remark-lint-maximum-line-length": "2.0.3",
    "remark-lint-no-empty-url": "2.0.1",
    "remark-lint-no-tabs": "2.0.1",
    "remark-lint-ordered-list-marker-value": "2.0.1",
    "remark-lint-unordered-list-marker-style": "2.0.1",
    "remark-preset-lint-recommended": "5.0.0",
    "remark-validate-links": "10.0.3"
  }
}
